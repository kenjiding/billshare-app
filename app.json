{
  "expo": {
    "name": "expo-test",
    "slug": "expo-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kenjiding.expotest"
    },
    "android": {
      "enableHermes": true,
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.kenjiding.expotest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "de8aaa3d-260b-48a6-b46d-8e7b23e46550"
      }
    }
  }
}